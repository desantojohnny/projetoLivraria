<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Document</title>
  <link rel="stylesheet" href="/stylesheets/style.css">
</head>
  <body>
  <%- include('header'); %>
  <%- include('categorias-princial'); %>
  <%- include('banner-central'); %>
  
  <!-- colocar os produtos em código que ele chame conforme o cliente abre a página -->
  
  <section>
    <div class="box-home-1">
        <b>Todos os Produtos</b>
    </div>
  </section>
 <!-- CONTAINER PRODUTO 01  -->
<section class="box-autor-home1">
    <% books.forEach(book => { %>
    <div class="box-autor-destaque1">
        <div class="box-prod-2" data-key="0">
            <a href>
                <div class="box-book">
                    <img src="<%=book.url_image%>" alt="" height="180vw" srcset="">
                </div>
            </a>
            <div class="book-item-price-2">R$<%=book.price%></div>
            <div class="book-item-autor-2"><em><%=book.authors%></em></div>
            <div class="book-item-titulo-2"><strong><%=book.title %></strong></div>
            <div class="book-item--add">COMPRAR</div>
        </div>
    </div> <% }); %>
</section>
  <%- include('footer'); %>

  
  </body>
</html>
